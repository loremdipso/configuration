{
	"vim.normalModeKeyBindings": [
		{
			"before": [" "],
			"after": ["o", "<Esc>"]
		},
		{
			"before": [";"],
			"after": [":"]
		}
	],
	"competitive-programming-helper.defaultLanguage": "C++",
	"explorer.confirmDelete": false,
	"competitive-programming-helper.firstTime": false,
	"competitive-programming-helper.saveLocation": "/home/madams/Projects/Topcoder/.garbage",
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.detectIndentation": false,
	"go.useLanguageServer": true,
	"editor.autoClosingBrackets": "never",
	"explorer.confirmDragAndDrop": false,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"rust-analyzer.inlayHints.enable": false,
	"editor.insertSpaces": false,
	"cSpell.userWords": [
		"actix",
		"Architected",
		"checklogin",
		"Cookeville",
		"devlog",
		"devs",
		"faceapi",
		"greenfielding",
		"hahaha",
		"halfnelson",
		"hoster",
		"hyperfixation",
		"loremdipso",
		"Middlewares",
		"octocrab",
		"postgres",
		"promisified",
		"résumé",
		"Roadmap",
		"scrollable",
		"signup",
		"Skunkworks",
		"struct",
		"taggenator",
		"tauri",
		"Telehealth",
		"upgrader"
	],
	"compile-hero.scss-output-directory": ".",
	"html.format.indentInnerHtml": true,

	"prettier.useTabs": true,
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"files.exclude": {
		"**/node_modules": true
	},
	"vetur.format.defaultFormatterOptions": {
		"prettier": {
			"singleQuote": true,
			"semi": false
		}
	},
	"[typescript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[javascript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"spellright.language": ["English"],
	"spellright.documentTypes": ["markdown", "latex", "plaintext"],
	"workbench.activityBar.visible": true,
	"cSpell.enableFiletypes": [
		"!css",
		"!go",
		"!python",
		"!rust",
		"!scss",
		"!typescriptreact"
	],
	"breadcrumbs.enabled": false,
	"workbench.statusBar.visible": true,
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust"
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.formatOnSave": true,
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"javascript.updateImportsOnFileMove.enabled": "always",
	"prettier.tabWidth": 4,
	"vetur.format.options.useTabs": true,
	"vetur.format.options.tabSize": 4,
	"python.formatting.autopep8Args": [
		"--indent-size=4",
		"--ignore=W191,E101,E111"
	],
	"editor.minimap.enabled": false,
	"telemetry.enableTelemetry": false,
	"gitlens.advanced.telemetry.enabled": false,
	"code-runner.enableAppInsights": false,
	"telemetry.enableCrashReporter": false,
	"js/ts.implicitProjectConfig.experimentalDecorators": true,
	"html.autoClosingTags": false,
	"angular.experimental-ivy": true,
	"redhat.telemetry.enabled": false,
	"svelte.enable-ts-plugin": true,
	"window.menuBarVisibility": "toggle"
}
